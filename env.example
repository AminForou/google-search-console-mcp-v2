# Google OAuth Configuration (Required)
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret

# OAuth Callback URL (must match Google Cloud Console)
GOOGLE_REDIRECT_URI=https://your-domain.com/oauth/callback

# Your server's public URL (for displaying in success page)
BASE_URL=https://your-domain.com

# Session secret key (auto-generated if not set)
SECRET_KEY=your-secret-key-here

# Database path
DATABASE_PATH=gsc_tokens.db

